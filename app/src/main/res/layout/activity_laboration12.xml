<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Emil"
            android:ems="10"
            android:id="@+id/editText2"
            app:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
    />
    <TextView
            android:text="Namn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            tools:layout_editor_absoluteX="112dp" android:id="@+id/editText4"
            android:text="htnshthr" app:layout_constraintTop_toBottomOf="@+id/editText2"/>
    <TextView
            android:text="Lösenord"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="56dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            tools:layout_editor_absoluteX="112dp" android:id="@+id/editText5"
            android:text="test@example.com" app:layout_constraintTop_toBottomOf="@+id/editText4"/>
    <TextView
            android:text="Epost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="96dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView3"/>
    <SeekBar
            android:layout_width="215dp"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="112dp" android:id="@+id/seekBar" android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/editText5"/>
    <TextView
            android:text="Ålder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="135dp"
            tools:layout_editor_absoluteX="14dp" android:id="@+id/textView4"/>
</android.support.constraint.ConstraintLayout>